<div class="container m-10">
 <h2>Check out our Games!!</h2>
</div>
<div class="container m-8">
    <div class="row">
      <% @games.each do |game| %>
        <div class="col-md-3">
          <div class="card h-100 text-center mb-5">
            <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title"><%= game.name %></h5>
              <p class="card-text"><%= game.description %></p>
              <p class="card-text"><small class="text-muted"><i class="fas fa-user"> - <%= game.number_players %></i> <i class="fas fa-dollar-sign"> - <%= game.day_price %></i></small></p>
              <button class="btn btn-outline-primary my-2 my-sm-0 mr-3" type="submit"><%= link_to 'More Details', new_game_booking_path(game) %></button>
            </div>
          </div>
        </div>
      <% end %>
      <%= link_to 'Add a new game', new_game_path %>
    </div>
</div>
